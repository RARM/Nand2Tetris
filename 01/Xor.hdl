/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a,		b=b, 		out=nab);
    Nand(a=a,		b=nab,		out=nanab);
    Nand(a=nab,		b=b,		out=nbnab);
    Nand(a=nanab,	b=nbnab,	out=out);
}
